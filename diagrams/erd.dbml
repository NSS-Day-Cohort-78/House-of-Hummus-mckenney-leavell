Table entrees {
    id int [pk]
    name varchar
    price int
}

Table vegetables {
    id int [pk]
    type varchar
    price int
}

Table sides {
    id int
    title varchar
    price int
}

Table purchases {
    id int [pk]
    entreeId int [ref: > entrees.id]
    vegetableId int [ref: > vegetables.id]
    sideId int [ref: > sides.id]
}
